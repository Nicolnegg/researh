
[kernel]
isa = x86
entrypoint = main
[x86]
protected-mode = true
[sse]
enabled = true
depth = 10000
directives = 0x8049875 reach;0x80498c0 cut;0x80498a5 cut;0x804987c cut

robust = true
robust-merge = yes
robust-mode = validation
[fml]
solver = z3
universal-mode = quantifier
unquantify-memory = true
optim-all = true
