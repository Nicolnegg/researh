[kernel]
isa = x86
entrypoint = main

[x86]
protected-mode = true

[sse]
enabled = true
depth = 10000
directives = 0x80498c0 cut;0x804987c cut;0x804988b cut;0x8049875 reach;0x80498ae assume (@[0x080e3f57,1] = @[0x080e3f51,1]);0x80498ae assume (@[0x080e3f50,1] <> @[0x080e3f55,1]);0x80498ae assume (@[0x080e3f57,1] = @[0x080e3f52,1])
robust = true
robust-merge = yes
robust-mode = validation

[fml]
solver = z3
universal-mode = quantifier
unquantify-memory = true
optim-all = true

