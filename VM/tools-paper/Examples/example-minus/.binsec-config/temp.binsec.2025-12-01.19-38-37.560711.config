[kernel]
isa = x86
entrypoint = main

[x86]
protected-mode = true

[sse]
enabled = true
depth = 10000
directives = 0x8049897 cut;0x804987c cut;0x80498ac cut;0x8049875 reach;0x8049885 assume (@[0xfff7ffdc,1] = @[0xfff7ffdd,1]);0x8049885 assume (@[0xfff7ffdf,1] = @[0xfff7ffdb,1])
robust = true
robust-merge = yes
robust-mode = validation

[fml]
solver = z3
universal-mode = quantifier
unquantify-memory = true
optim-all = true

